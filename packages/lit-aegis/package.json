{
	"name": "@roenlie/lit-aegis",
	"version": "1.0.41",
	"description": "Dependency injection framework based on LIT and inversify.",
	"author": {
		"name": "Kristoffer Roen-Lie",
		"url": "https://github.com/RoenLie"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RoenLie/mimic.git"
	},
	"bugs": {
		"url": "https://github.com/RoenLie/mimic/issues"
	},
	"homepage": "https://github.com/RoenLie/mimic#readme",
	"files": [
		"./dist/lib",
		"./src",
		"./LICENSE",
		"./package.json",
		"./readme.md"
	],
	"type": "module",
	"main": "./dist/lib/index.js",
	"types": "./dist/lib/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/lib/index.d.ts",
			"default": "./dist/lib/index.js"
		},
		"./*": "./dist/lib/*"
	},
	"scripts": {
		"test": "vitest",
		"dev": "pnpm run --parallel \"/^watch-.*/\"",
		"watch-js": "vite build --watch",
		"watch-ts": "tsc --project ./src/tsconfig.json --watch",
		"build": "pnpm run --sequential \"/^build-.*/\"",
		"build-clean": "rimraf dist",
		"build-indexes": "package-toolbox build-indexes",
		"build-exports": "package-toolbox build-exports",
		"build-js": "vite build",
		"build-ts": "tsc --project ./src/tsconfig.json",
		"updatedeps": "pnpm update --latest",
		"increment-version": "package-toolbox increment-version"
	},
	"dependencies": {
		"@roenlie/mimic-core": "workspace:^",
		"@roenlie/reflect-metadata": "^1.0.3",
		"deepmerge-ts": "^5.1.0",
		"inversify": "^6.0.2"
	},
	"peerDependencies": {
		"lit": "*"
	},
	"devDependencies": {
		"lit": "^3.1.1"
	},
	"sideEffects": [
		"./dist/lib/annotations/annotations.js",
		"./dist/lib/annotations/static-injector.js"
	]
}