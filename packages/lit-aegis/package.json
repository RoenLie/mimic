{
	"name": "@roenlie/lit-aegis",
	"version": "1.0.20",
	"description": "Dependency injection framework based on LIT and inversify.",
	"author": {
		"name": "Kristoffer Roen-Lie",
		"url": "https://github.com/RoenLie"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RoenLie/mimic.git"
	},
	"bugs": {
		"url": "https://github.com/RoenLie/mimic/issues"
	},
	"homepage": "https://github.com/RoenLie/mimic#readme",
	"type": "module",
	"types": "./dist/index.d.ts",
	"main": "./dist/index.js",
	"exports": {
		"./*": "./dist/*"
	},
	"scripts": {
		"test": "vitest",
		"test:dom": "vitest --dom",
		"dev": "pnpm vite --config ./docs/vite.config.ts",
		"build": "pnpm build:prepare && pnpm build:types && vite build",
		"build:prepare": "rimraf dist",
		"build:index": "package-toolbox build-indexes",
		"build:exports": "package-toolbox build-exports",
		"build:types": "tsc --project ./src/tsconfig.json",
		"updatedeps": "pnpm update --latest",
		"increment-version": "package-toolbox increment-version"
	},
	"dependencies": {
		"@abraham/reflection": "^0.12.0",
		"@roenlie/mimic-core": "workspace:^",
		"deepmerge-ts": "^5.1.0",
		"inversify": "^6.0.2"
	},
	"peerDependencies": {
		"lit": "*"
	},
	"devDependencies": {
		"@roenlie/mirage-docs": "^1.2.28",
		"happy-dom": "^12.10.3",
		"lit": "^3.1.0"
	},
	"sideEffects": false
}