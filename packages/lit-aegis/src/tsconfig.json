{
	"extends": "@roenlie/tsconfig",
	"compilerOptions": {
		"outDir": "../dist/lib",
		"rootDir": ".",
		"baseUrl": ".",
		"noEmit": false,
		"emitDeclarationOnly": true,
		"incremental": true,
		"tsBuildInfoFile": "../dist/.tsbuildinfo",
		"types": [
			"vite/client"
		]
	},
	"include": [
		"./**/*.ts",
		"./**/*.d.ts",
		"./**/*.js",
	],
	"exclude": [
		"./**/*.test.ts",
		"./**/*.bench.ts",
	]
}