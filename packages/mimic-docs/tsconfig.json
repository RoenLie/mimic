{
	"extends": "@roenlie/tsconfig",
	"compilerOptions": {
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"target": "ESNext",
		"rootDir": ".",
		"baseUrl": ".",
		"noEmit": true,
		"emitDeclarationOnly": false,
		"incremental": false,
		"types": [
			"vite/client",
		]
	},
	"include": [
		"./src/**/*.ts",
		"./src/**/*.js",
	],
	"exclude": []
}